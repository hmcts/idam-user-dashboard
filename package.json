{
  "name": "idam-user-dashboard",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production ./node_modules/.bin/ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "nodemon --config nodemon.json",
    "lint": "eslint . --ext .ts",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "playwright:install": "playwright install",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.smoke.config.js",
    "test:crossbrowser": "npm-run-all -scl playwright:install test:crossbrowser:e2e test:crossbrowser:report",
    "test:crossbrowser:e2e": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run-multiple --all --config src/test/cross-browser/saucelabs.conf.js --grep @CrossBrowser",
    "test:crossbrowser:report": "allure generate functional-output/cross-browser/reports/* -c -o functional-output/cross-browser/allure ",
    "test:functional": "npm-run-all -scl playwright:install test:functional:min test:functional:report test:pa11y",
    "test:fullfunctional": "npm-run-all -scl playwright:install test:functional:all test:functional:report test:pa11y",
    "test:functional:min": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run-workers 1 parallel --config ./src/test/functional/codecept.conf.js --grep \"(?=.*)^(?!.*@Nightly)\"",
    "test:functional:all": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run-workers 2 parallel --config ./src/test/functional/codecept.conf.js",
    "test:functional:report": "allure generate functional-output/functional/reports -c -o functional-output/functional/allure",
    "test:a11y": "echo 'nope' && exit 0",
    "test:pa11y": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.a11y.config.js --maxWorkers 1",
    "sonar-scan": "sonar-scanner",
    "fortifyScan": "./java/gradlew -p java fortifyScan",
    "cichecks": "yarn && npm-run-all -pl build lint test"
  },
  "dependencies": {
    "@hmcts/info-provider": "^1.1.0",
    "@hmcts/nodejs-healthcheck": "^1.8.0",
    "@hmcts/nodejs-logging": "^4.0.4",
    "@hmcts/properties-volume": "^1.0.0",
    "@types/config": "^3.0.0",
    "@types/connect-redis": "^0.0.23",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/es6-promisify": "^6.0.1",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/glob": "^8.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest-when": "^3.5.2",
    "@types/json2csv": "^5.0.3",
    "@types/lodash": "^4.14.197",
    "@types/node": "^17.0.45",
    "@types/nunjucks": "^3.2.3",
    "@types/require-directory": "^2.1.2",
    "@types/serve-favicon": "^2.5.4",
